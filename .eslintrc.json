{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["xo", "prettier"],
  "overrides": [
    {
      "extends": ["xo-typescript", "prettier"],
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "no-unused-vars": "error",
        "no-console": "error"
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  }
}
